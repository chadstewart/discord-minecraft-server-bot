{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "ts-node",
      "type": "node",
      "smartStep": true,
      "request": "launch",
      "runtimeExecutable": "ts-node",
      "args": ["src/index.ts"],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    }
  ]
}